{
  "name": "tripshare",
  "version": "1.0.0",
  "description": "Plateforme sociale de voyage nouvelle génération avec IA",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "clear": "expo start --clear",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "depcheck": "npx depcheck",
    "ts-prune": "npx ts-prune",
    "fix-deps": "expo doctor --fix-dependencies",
    "clean-all": "expo start --clear && npm run clear-cache",
    "clear-cache": "rm -rf node_modules/.cache && rm -rf .expo && rm -rf dist",
    "reset-project": "npm run clear-cache && npm install && expo start --clear",
    "debug-hermes": "expo start --dev-client --clear",
    "security-audit": "npm audit --audit-level=high"
  },
  "dependencies": {
